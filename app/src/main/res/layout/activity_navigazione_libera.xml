<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigazionelibera_constraint"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <fragment xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:map="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="337dp"
        android:layout_height="356dp"
        app:layout_constraintBottom_toBottomOf="@id/callButton1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        map:layout_constraintHorizontal_bias="0.51"
        map:layout_constraintVertical_bias="0.131"
        tools:context="it.univaq.casatracking.NavigazioneLiberaActivity" />

    <ImageButton
        android:id="@+id/callButton1"
        android:layout_width="113dp"
        android:layout_height="wrap_content"
        android:contentDescription="@string/tel"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@id/map"
        app:layout_constraintVertical_bias="0.965"
        app:srcCompat="@android:drawable/sym_action_call" />

</android.support.constraint.ConstraintLayout>